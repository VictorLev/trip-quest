  <div class="d-flex flex-column justify-content-center">
    <div class="fs-6 mb-1 ms-1"><strong><%= user.first_name %> <%= user.last_name %></strong></div>

    <% if user.total_rewards / 1000 >= 8 && user.total_rewards / 1000 < 9 %>
      <div class="progress d-flex rounded-4 mx-2 my-2" style="width: 90%;">
        <div class="progress-bar bg-warning progress-bar-striped" role="progressbar" style="width: 25%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
          <p><strong>level <%= user.total_rewards / 1000 %></strong></p>
        </div>
      </div>
      <div class="d-flex pt-1">
        <% (user.total_rewards / 1000).times do %>
          <i class="fa-solid fa-star fa-lg mx-1 my-2"></i>
        <% end %>
        <% (9 - (user.total_rewards / 1000)).times do %>
          <i class="fa-regular fa-star fa-lg mx-1 my-2"></i>
